// dbx.v1 golang irreparabledb.dbx .
model irreparabledb (
	key segmentpath

model bwagreement (
	key signature 

	field signature blob 

	field data blob
	
	field created_at timestamp ( autoinsert )
)

create bwagreement ( )
delete bwagreement ( where bwagreement.signature = ? )
read one (
  select bwagreement 
  where  bwagreement.signature = ?
)
read limitoffset (
	select bwagreement
)
read all (
	select bwagreement
)
read all (
	select bwagreement
	where  bwagreement.created_at > ?
)