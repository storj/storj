    "vuetify": "3.3.2"
  },
  "devDependencies": {
    "@types/filesystem": "0.0.32",
    "@types/node": "16.18.14",
    "@types/qrcode": "1.5.0",
    "@typescript-eslint/eslint-plugin": "5.59.5",
    "@typescript-eslint/parser": "5.59.5",
    "@vitejs/plugin-vue": "4.2.3",
    "@vue/eslint-config-typescript": "11.0.3",
    "eslint": "8.40.0",
