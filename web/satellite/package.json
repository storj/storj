    "@aws-sdk/signature-v4": "3.338.0",
    "@hcaptcha/vue3-hcaptcha": "1.2.1",
    "@mdi/font": "7.0.96",
    "bip39-english": "2.5.0",
    "chart.js": "4.2.1",
    "graphql": "15.3.0",
